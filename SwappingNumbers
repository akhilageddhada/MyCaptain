#include<stdio.h>
int main()
{
    int a,b;
    printf("enter a number:\n");
    scanf("%d",&a);
    printf("enter another number:\n");
    scanf("%d",&b);
    printf("numbers before swapping are:%d,%d\n",a,b);
    //swapping
    a=a+b;
    b=a-b;
    a=a-b;
    printf("numbers after swapping are:%d,%d\n",a,b);
    return 0;
}
